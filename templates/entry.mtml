	</div><!-- .secondary -->

	</div><!-- .sidebar -->
	
	
		<div id="content" class="site-content">

	<div id="primary" class="content-area">
		<main id="main" class="site-main">

	
		
<article id="post-<$mt:EntryID$>" class="post-<$mt:EntryID$> post type-post status-publish format-standard hentry category-<mt:EntryPrimaryCategory><$mt:CategoryLabel$></mt:EntryPrimaryCategory>">
	
	<header class="entry-header">
		<h2 class="entry-title"><$mt:EntryTitle$></h2>
	</header><!-- .entry-header -->

	<div class="entry-content">
	<$mt:EntryBody$>
    <$mt:EntryMore$>
	</div><!-- .entry-content -->

	
	<footer class="entry-footer">
		<span class="posted-on"><span class="screen-reader-text">发布于 </span><a href="<$mt:EntryPermalink encode_html="1"$>" rel="bookmark"><time class="entry-date published updated" datetime="<$mt:EntryDate format_name="iso8601"$>"><$mt:EntryDate format="%x"$></time></a></span>
		<span class="byline"><span class="author vcard"><span class="screen-reader-text">作者 </span><a class="url fn n" href="<$mt:BlogURL encode_html="1"$>">Zheng</a></span></span>
<mt:IfArchiveTypeEnabled archive_type="Category">
  <mt:If tag="EntryPrimaryCategory">	
		<span class="cat-links"><span class="screen-reader-text">分类 </span><mt:EntryPrimaryCategory><a href="<$mt:CategoryArchiveLink$>"><$mt:CategoryLabel$></a></mt:EntryPrimaryCategory></span>
		  </mt:If>
</mt:IfArchiveTypeEnabled>
	</footer>
</article><!-- #post-## -->

